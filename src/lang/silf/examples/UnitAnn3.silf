function lb2kg(w)
  pre(UNITS): @unit(w) = $lb;
  post(UNITS): @unit(@result) = $kg;
begin
  return 10 * w / 22;
end

function main(void)
begin
  var $lb projectileWeight;
  var $kg weightInKg;
  projectileWeight := 5;
  weightInKg := lb2kg(projectileWeight);
  write projectileWeight;
  write weightInKg;
end